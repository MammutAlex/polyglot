{
  "name": "mammut-alex/polyglot",
  "description": "Organization of DB multilingualism in laravel, easy to switch",
  "keywords": [
    "laravel",
    "DB",
    "translation",
    "simple"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "MammutCris",
      "email": "mammutcris@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "laravel/laravel": "dev-develop"
  },
  "autoload": {
    "psr-4": {
      "MammutAlex\\Polyglot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MammutAlex\\Polyglot\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
